{
  "name": "vue3-admin-simple",
  "private": true,
  "version": "0.1.0",
  "license": "MIT",
  "description": "A vue3 admin template with Ant-Design-Vue",
  "author": "AodaZhang <zhangxinyu1989@foxmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/AodaZhang/vue-admin-simple"
  },
  "scripts": {
    "start": "vue-cli-service serve --mode dev",
    "build:uat": "vue-cli-service build --mode uat",
    "build:prod": "vue-cli-service build --mode prod",
    "lint:fix": "vue-cli-service lint --fix",
    "test:unit": "vue-cli-service test:unit --watch",
    "commit": "git-cz",
    "changelog": "npm --no-git-tag-version version 0.1.1 && conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "dependencies": {
    "@ant-design-vue/use": "^0.0.1-alpha.9",
    "@ant-design/icons-vue": "^6.0.1",
    "ant-design-vue": "^2.0.1",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "dayjs": "^1.10.4",
    "nprogress": "^0.2.0",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@types/jest": "^24.0.19",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/babel-plugin-jsx": "^1.0.3",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^2.0.0-0",
    "antd-dayjs-webpack-plugin": "^1.0.6",
    "autoprefixer": "^9.8.6",
    "babel-plugin-import": "^1.13.3",
    "body-parser": "^1.19.0",
    "commitizen": "^4.2.3",
    "compression-webpack-plugin": "^6.1.1",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.0.0-0",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "typescript": "~3.9.3",
    "vue-jest": "^5.0.0-0",
    "webpack-bundle-analyzer": "^3.9.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -E GIT_PARAMS"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx,vue}": [
      "vue-cli-service lint --fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}